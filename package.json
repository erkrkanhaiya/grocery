{
  "name": "Grocery",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean": "npm start --reset-cache",
    "clean-node": "npm cache clean --force",
    "clean-android": "cd android && ./gradlew clean && cd ../",
    "clean-ios": "cd ios && xcodebuild clean && cd ../",
    "clean-npm": "npm cache clean --force",
    "release-android": "cd android && ./gradlew assembleRelease",
    "debug-android": "cd android && ./gradlew assembleDebug",
    "android": "yarn rnuc .env && react-native run-android",
    "ios": "yarn rnuc .env && react-native run-ios",
    "build-apk": "yarn rnuc .env && cd android && ./gradlew clean && ./gradlew assembleRelease && cd ..",
    "package-apk": "yarn rnuc .env && cd android && ./gradlew clean && ./gradlew bundleRelease && cd ..",
    "start": "react-native start --reset-cache",
    "android-release": "react-native bundle --platform android --dev false --entry-file index.android.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "postinstall": "yarn rnuc .env && npx pod-install"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/checkbox": "^0.5.9",
    "@react-native-community/netinfo": "^7.1.5",
    "@react-native-firebase/app": "^14.1.0",
    "@react-native-firebase/auth": "^10.3.1",
    "@react-native-firebase/firestore": "^14.1.0",
    "@react-native-masked-view/masked-view": "^0.2.6",
    "@react-navigation/bottom-tabs": "^6.0.9",
    "@react-navigation/drawer": "^6.1.8",
    "@react-navigation/material-bottom-tabs": "^6.0.9",
    "@react-navigation/material-top-tabs": "^6.0.6",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/native-stack": "^6.2.5",
    "axios": "^0.24.0",
    "moment": "^2.29.1",
    "react": "17.0.2",
    "react-native": "0.66.3",
    "react-native-carousel-control": "^2.0.1",
    "react-native-device-info": "^8.4.8",
    "react-native-fast-image": "^8.5.11",
    "react-native-flash-message": "^0.2.0",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-input-spinner": "^1.7.12",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-localize": "^2.1.5",
    "react-native-otp-textinput": "^0.0.8",
    "react-native-pager-view": "^5.4.9",
    "react-native-paper": "^4.10.1",
    "react-native-pixel-perfect": "^1.0.2",
    "react-native-reanimated": "1.13.1",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.9.0",
    "react-native-simple-toast": "^1.1.3",
    "react-native-snackbar-component": "^1.1.12",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-splash-screen": "^3.3.0",
    "react-native-swiper": "^1.6.0",
    "react-native-tab-view": "^3.1.1",
    "react-native-ultimate-config": "3.2.4",
    "react-native-vector-icons": "^9.0.0",
    "react-query": "^3.16.0",
    "react-redux": "^7.2.4",
    "recoil": "^0.3.1",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/runtime": "^7.16.3",
    "@react-native-community/eslint-config": "^3.0.1",
    "babel-jest": "^27.3.1",
    "eslint": "^8.3.0",
    "jest": "^27.3.1",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-test-renderer": "17.0.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
